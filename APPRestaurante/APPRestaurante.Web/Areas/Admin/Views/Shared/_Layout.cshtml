@{

    Layout = null;
}

<!DOCTYPE html>

<html lang="es">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
    <title>@ViewBag.Title | APPRestaurante v 1.0</title>
    <!--Styles-->
    <link href="~/Areas/Admin/Assets/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Areas/Admin/Assets/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Areas/Admin/Assets/css/ace-fonts.css" rel="stylesheet" />
    <link href="~/Areas/Admin/Assets/css/ace.css" class="ace-main-stylesheet" id="main-ace-style" rel="stylesheet" />
    <link href="~/Areas/Admin/Assets/css/bootstrap-datetimepicker.css" rel="stylesheet" />
    <script src="~/Areas/Admin/Assets/js/ace-extra.js"></script>
    <script src="~/Areas/Admin/Assets/js/jquery.js"></script>
    <script src="~/Areas/Admin/Assets/js/bootstrap-datepicker.js"></script>
</head>
<body class="no-skin">

    @Html.Partial("~/Areas/Admin/Views/Shared/_Navbar.cshtml")

    <div class="main-container container" id="main-container">
        <script type="text/javascript">
            try { ace.settings.check('main-container', 'fixed') } catch (e) { }
        </script>

        @Html.Partial("~/Areas/Admin/Views/Shared/_Sidebar.cshtml")

        <div class="main-content">
            <div class="main-content-inner">

                @Html.Partial("~/Areas/Admin/Views/Shared/_Breadcrumbs.cshtml")

                <div class="page-content">

                    @RenderBody()

                </div>
            </div>
        </div>

        @Html.Partial("~/Areas/Admin/Views/Shared/_Footer.cshtml")

    </div>

    <script type="text/javascript">
        if ('ontouchstart' in document.documentElement) document.write("<script src='@Url.Content("~/Areas/Admin/Assets/js/jquery.mobile.custom.js")'>" + "<" + "/script>");
    </script>
    <script src="~/Areas/Admin/Assets/js/bootstrap.js"></script>
    <!--[if lte IE 8]>
        <script src="~/Areas/Admin/Assets/js/excanvas.js"></script>
    <![endif]-->
    <script src="~/Areas/Admin/Assets/js/jquery-ui.custom.js"></script>
    <script src="~/Areas/Admin/Assets/js/jquery.ui.touch-punch.js"></script>
    <script src="~/Areas/Admin/Assets/js/jquery.easypiechart.js"></script>
    <script src="~/Areas/Admin/Assets/js/jquery.sparkline.js"></script>
    <script src="~/Areas/Admin/Assets/js/flot/jquery.flot.js"></script>
    <script src="~/Areas/Admin/Assets/js/flot/jquery.flot.pie.js"></script>
    <script src="~/Areas/Admin/Assets/js/flot/jquery.flot.resize.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/elements.scroller.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/elements.colorpicker.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/elements.fileinput.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/elements.typeahead.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/elements.wysiwyg.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/elements.spinner.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/elements.treeview.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/elements.wizard.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/elements.aside.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/ace.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/ace.ajax-content.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/ace.touch-drag.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/ace.sidebar.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/ace.sidebar-scroll-1.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/ace.submenu-hover.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/ace.widget-box.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/ace.settings.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/ace.settings-rtl.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/ace.settings-skin.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/ace.widget-on-reload.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/ace.searchbox-autocomplete.js"></script>

    <link href="~/Areas/Admin/Assets/css/ace.onpage-help.css" rel="stylesheet" />
    <link href="~/Areas/Admin/Assets/css/sunburst.css" rel="stylesheet" />

    <script src="~/Areas/Admin/Assets/js/ace/elements.onpage-help.js"></script>
    <script src="~/Areas/Admin/Assets/js/ace/ace.onpage-help.js"></script>
    <script src="~/Areas/Admin/Assets/js/rainbow.js"></script>
    <script src="~/Areas/Admin/Assets/js/generic.js"></script>
    <script src="~/Areas/Admin/Assets/js/html.js"></script>
    <script src="~/Areas/Admin/Assets/js/css.js"></script>
    <script src="~/Areas/Admin/Assets/js/javascript.js"></script>

    <script>
            var layoutData = {
                urlProvider: {
                    cerrarSesion: '@Url.Action("CerrarSession", "Login")',
                    redirectLogin: '@Url.Action("Index", "Login")',
                    cargaData: '@Url.Action("CargaDatos", "Carga")'
                }
            };
    </script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/layout.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/general.js")"></script>


</body>
</html>
